{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./components/*"],
      "@utils/*": ["./utils/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/middleware.js", "src/interfaces/NotificationCount.interfacets", "server.js", "src/socket.ts", "src/contexts/HeaderHeightContext.js", "src/components/Map/Map.js", "src/components/Map/Map.jsx", "src/components/Chat/Chat.Listtsx", "src/types/contexts/chatContext.type", "src/contexts/ChatContext/ChatContext.ts"],
  "exclude": ["node_modules"]
}
